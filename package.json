{
  "name": "sn-cloudinary-mcp",
  "version": "0.1.0",
  "private": false,
  "type": "module",
  "description": "Cloudinary MCP server for Shark Ninja Data.",
  "scripts": {
    "dev": "ts-node --esm src/server.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "cloudinary": "^1.41.0",
    "ajv": "^8.12.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "typescript": "^5.5.0",
    "ts-node": "^10.9.2",
    "eslint": "^9.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.2.0"
  },
  "engines": {
    "node": ">=18"
  }
}